<%- include('header'); -%>
    <div class="box" id="heading">
        <h1><%= KindOfDay %></h1>
    </div>
    <div class="box">
        <% for(let i=0; i<newListItem.length; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%= newListItem[i] %></p>
            </div>     
        <% } %>
        <form action="/" method="post" class="item">
            <input type="text" name="newItem" placeholder="New Item" autocomplete="false">
            <button type="submit" name="button">+</button>
        </form>
    </div>
<%- include('footer'); -%>